        <Button x:Name="GetInfo" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="232,340,0,0"/>
        <TextBox x:Name="DogName" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="162,161,0,0" TextChanged="DogName_TextChanged"/>
